//투표를 하는 경우
var GoPoll = Class.create({
	initialize : function(uid){
		this.mode = 'go_poll';
		this.uid = uid;
		this.url = "/no_regist/regist_poll.php";
		this.code = '';
		this.check_code = [];
	},
	go_poll : function(code){
		if(!!this.uid==false){
			alert("로그인후 사용가능합니다.");
			return;
		}		
		
		if(!!code==false) {
			alert(Config.error['empty_param']);
			return;
		}
		
		if(this.check_code.indexOf(code) > -1){
			alert(Config.error['already_check']);
			return;
		}		
		
		this.code = code;

		var name_val = "poll_answer_" +code;
		var answer;
		var checked;
		var obj =document.getElementsByName(name_val);
		for(var i = 0;i<obj.length;i++){
			if(obj[i].checked == true){
				answer = i;
				checked = true;
			}
		}

		if(!checked) {
			alert("답변을 선택하여 주시기 바랍니다.");
			return;
		}

		

		var param = {'code' : code, 'answer' : answer, 'uid' : this.uid, 'mode': this.mode};
		//alert($H(param).toQueryString());

		new GoAjax(this, this.url, param);
	},
	get_result : function(xmlHttp, xJson){
		var result = netfu_util.parseAjax(xmlHttp);
		if(result.result=="false"){
			if(result.error) {
				alert(Config.error[result.error]);
			} else {
				alert("실패하였습니다.");
			}
			return;
		}
		
		alert("투표결과가 반영되었습니다.");
		this.check_code.push(this.code);
	}
});

var goPoll = new GoPoll('');


</script>

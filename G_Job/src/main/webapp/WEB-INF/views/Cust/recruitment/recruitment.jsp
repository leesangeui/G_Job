<script>
var SendSms = Class.create({
	initialize: function(is_member){
		this.is_member = is_member;
		this.mode = "insert";
		this.url = "/no_regist/regist_sms.php";	
		//this.clear_field();	
	},
	send_sms : function(){
		if(!!this.is_member == false){
			alert("로그인을 하여 주시기 바랍니다.");
			this.clear_field();
			return false;
		}	
		
		var name_ = $("send_sms_name");
		var phone= $("send_sms_phone");
		var msg = $("send_sms_content");	
		
		if(!!name_.value == false){
			alert("이름을 입력하여 주시기 바랍니다.");
			name_.focus();
			//this.clear_field();
			return;
		}	
		if(!isValidHPhone(phone,'', true)) {
			alert("핸프폰 번호가 정확하지 않습니다.");
			phone.focus();
			//this.clear_field();
			return;
		} 
		
		if(!!msg.value ==false){
			alert("내용을 입력하여 주시기 바랍니다.");
			msg.focus();
			//this.clear_field();
			return;
		}
		
		var param = {'mode': this.mode, 'name' : name_.value, 'phone' : phone.value, 'msg' : encodeURIComponent(msg.value)};
		
		new GoAjax(this, this.url, param);
		
		
		
	}, 
	get_result : function(xmlHttp, xJson) {
		var result = netfu_util.parseAjax(xmlHttp);
		
		if(result.result=="true") {
			alert("전송되었습니다.");
		} else {
			alert("전송이 실패하였습니다.\n메일로 문의 바랍니다.");
		}
		
		this.clear_field();
	},
	clear_field: function(){
		$("send_sms_name").value=""; 
		$("send_sms_phone").value="";
		$("send_sms_content").value="";	
	}
});

var send_sms = new SendSms(0);
</script>


CREATE TABLE project_member
(
    nums               NUMBER(11)       PRIMARY KEY ,     
    userid              varchar2(20)     , 
    passwd           varchar2(100)    , 
    password_que     varchar2(20)     DEFAULT NULL, 
    password_chk     varchar2(100)    DEFAULT NULL, 
    junnum           varchar2(13)     DEFAULT NULL, 
    uname             varchar2(20)     DEFAULT NULL, 
    nick_name        varchar2(10)     DEFAULT NULL, 
    phone            varchar2(13)     DEFAULT NULL, 
    hphone           varchar2(13)     DEFAULT NULL, 
    birth            varchar2(12)     DEFAULT NULL, 
    sex              varchar2(5)      DEFAULT NULL, 
    use_mail         varchar2(3)      DEFAULT NULL, 
    use_sms          varchar2(3)      DEFAULT NULL, 
    email            varchar2(50)     DEFAULT NULL, 
    post             varchar2(10)     DEFAULT NULL, 
    address1        varchar2(100)    DEFAULT NULL, 
    address2         varchar2(100)    DEFAULT NULL, 
    wdate            TIMESTAMP       DEFAULT NULL , 
    login_date       TIMESTAMP        DEFAULT NULL , 
    home             varchar2(100)    DEFAULT NULL  , 
    photo            varchar2(100)    DEFAULT NULL  , 
    p_chk            varchar2(3)      DEFAULT NULL , 
    typical             varchar2(50)     DEFAULT NULL , 
    last_school      varchar2(100)    DEFAULT NULL  , 
    high_school      varchar2(200)    DEFAULT NULL  , 
    university       varchar2(200)    DEFAULT NULL  , 
    lefted             varchar2(3)      DEFAULT NULL  , 
    secession        varchar2(200)    DEFAULT NULL  , 
    l_date           TIMESTAMP        DEFAULT NULL , 
    hit              NUMBER(10)         DEFAULT 0 
      
);